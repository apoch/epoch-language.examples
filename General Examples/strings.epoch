entrypoint : () -> ()
{
	string(test, concat(array("The ", "quick ", "brown ", "fox ", "jumped over the lazy sle", "eping", " dog.")))
	debugwritestring(test)

	string(test2, "test")
	test2 ;= " two"
	debugwritestring(test2)

	integer(strlen, length(test))
	debugwritestring(cast(string, strlen))	
}

