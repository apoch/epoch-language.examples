//
// IFTEST.EPOCH
//
// Test of chained if/elseif/else blocks
//

entrypoint : () -> ()
{
	debugwritestring("Enter a number:")
	integer(foo, cast(integer, debugreadstring()))

	if(foo == 0)
	{
		debugwritestring("You entered zero")
	}
	elseif(foo == 1)
	{
		debugwritestring("You entered one")
	}
	elseif(foo == 2)
	{
		debugwritestring("You entered two")
	}
	else
	{
		debugwritestring("You entered something else")
	}
}
