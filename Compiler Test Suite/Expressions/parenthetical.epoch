//
// PARENTHETICAL.EPOCH
//
// Compiler test of parenthetical expressions
//


entrypoint :
{
	integer blargh = 1
	integer test = (blargh + 1) * 2

	foo(1 + (23 * bar() / 7) + baz())

	assert(test == 4)

	passtest()
}


foo : integer n
{
	assert(n == 42)
}


bar : -> 14

baz : -> -5

