//
// HIGHERORDERREF.EPOCH
//
// Compiler test for higher-order functions with reference-typed parameters
//


entrypoint :
{
	integer foo = 41
	mutate(foo, incrementer)
	
	assert(foo == 42)

	passtest()
}


incrementer : integer ref in
{
	++in
}


mutate : integer ref in, (func : integer ref)
{
	func(in)
}

