//
// ANONTEMP.EPOCH
//
// Compiler test of anonymous temporary structure variables
//

structure test :
	integer alpha,
	string beta


entrypoint :
{
	foo(test(42, "ding"))


	integer value = 42
	foo(test(value, "ding"))

	print("Pass")
}

foo : test param
{
	assert(param.alpha == 42)
	assert(param.beta == "ding")
}

